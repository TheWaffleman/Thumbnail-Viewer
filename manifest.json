{

  "manifest_version": 2,
  "name": "thumbnailViewer",
  "version": "1.3",

  "description": "Displays Youtube Thumbnail",


  "browser_specific_settings": {
    "gecko": {
      "id": "titan@owo.help",
	    "strict_min_version": "68.0"
    }
  },

  "background": {
	  "scripts": ["thumbnailViewer.js"]
    },

  "page_action": {
    "browser_style": true,
	  "default_icon": "icons/icon.png",
	  "default_title": "Show Youtube Thumbnail"
  },
  "permissions": [
    "activeTab"
  ]
}